<Window x:Class="WPFClient.View.DictionaryAdditionWizard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFClient.View"
        mc:Ignorable="d"
        Title="Dictionary Addition Wizard" Height="570" Width="970">
    <Grid Background="#FF1E1E1E">
        <Grid.RowDefinitions>
            <RowDefinition Height="106"></RowDefinition>
            <RowDefinition> </RowDefinition>
            <RowDefinition Height="70"> </RowDefinition>
        </Grid.RowDefinitions>

        <Canvas Grid.Row="0" Background="#FF232323">
            <Label Canvas.Left="10" Canvas.Top="10" Content="Welcome to the Dictionary Addition Wizard." Foreground="White" FontSize="20"></Label>
        </Canvas>
        
        
    </Grid>
</Window>
